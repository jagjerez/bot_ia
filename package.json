{
  "name": "bot-crypto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:generate:sqlite": "prisma generate --schema=prisma/schema.sqlite.prisma",
    "db:push": "prisma db push",
    "db:push:sqlite": "prisma db push --schema=prisma/schema.sqlite.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && pnpm db:seed",
    "db:reset:sqlite": "prisma db push --force-reset --schema=prisma/schema.sqlite.prisma && pnpm db:seed",
    "setup": "pnpm install && pnpm db:generate && pnpm db:push && pnpm db:seed",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf .next node_modules/.cache",
    "docker:build": "docker build -t crypto-bot .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local crypto-bot"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "aws-sdk": "^2.1692.0",
    "ccxt": "^4.3.25",
    "clsx": "^2.1.1",
    "lightweight-charts": "^4.1.3",
    "ml-matrix": "^6.10.4",
    "ml-random-forest": "^2.0.0",
    "mock-aws-s3": "^4.0.2",
    "next": "15.5.3",
    "nock": "^14.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rimraf": "^6.0.1",
    "simple-statistics": "^7.8.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
